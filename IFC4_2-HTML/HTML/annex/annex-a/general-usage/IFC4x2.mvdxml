            <Concept uuid="9d3bc6d5-0583-4dea-b75f-f32076bc60bb" name="Property Sets for Objects" status="sample" override="false">
              <Template ref="f74255a6-0c0e-4f31-84ad-24981db62461" />
              <TemplateRules operator="and">
                <TemplateRule Parameters="PsetName[Value]='Pset_StairCommon'" />
