        <ConceptRoot uuid="be243e0b-3cdf-4f68-8ea5-3c175dee9ca7" name="IfcShadingDevice" status="sample" applicableRootEntity="IfcShadingDevice">
          <Applicability uuid="00000000-0000-0000-0000-000000000000" status="sample">
