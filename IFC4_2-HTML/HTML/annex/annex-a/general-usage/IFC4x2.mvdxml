              <Template ref="ec9cfb04-adfe-4868-8168-d78fe995a913" />
              <TemplateRules operator="and">
                <TemplateRule Description="An idealized structural member corresponding to the plate." Parameters="Type[Type]='IfcStructuralSurfaceMember'" />
