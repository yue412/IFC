        <ConceptRoot uuid="19a7525c-1462-4a18-b8fd-6f49a3989791" name="IfcBuildingElementProxy" status="sample" applicableRootEntity="IfcBuildingElementProxy">
          <Applicability uuid="00000000-0000-0000-0000-000000000000" status="sample">
            <Template ref="41a9fa82-bb37-4792-9f8e-fb204f74cea7" />
