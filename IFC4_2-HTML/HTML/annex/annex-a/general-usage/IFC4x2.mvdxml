        <ConceptRoot uuid="a2b0737e-1ff6-4723-b6c3-301cdd2ba38a" name="IfcCurtainWall" status="sample" applicableRootEntity="IfcCurtainWall">
          <Applicability uuid="00000000-0000-0000-0000-000000000000" status="sample">
            <Template ref="6eaad21b-bac5-405e-9a2c-bcdc7f76a4f4" />
            <TemplateRules operator="and" />
          </Applicability>
