        <ConceptRoot uuid="2b58c57e-3ac1-4d90-b3f0-ac2daac69ed7" name="IfcOpeningElement" status="sample" applicableRootEntity="IfcOpeningElement">
          <Applicability uuid="00000000-0000-0000-0000-000000000000" status="sample">
            <Template ref="aead99d1-e4e5-487d-948c-3780dd5079a2" />
            <TemplateRules operator="and" />
          </Applicability>
