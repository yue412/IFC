                <TemplateRule Parameters="" />
                <TemplateRule Parameters="" />
              </TemplateRules>
            </Concept>
          </Concepts>
        </ConceptRoot>
        <ConceptRoot uuid="3ee15b73-6ba1-43cd-ae35-4a992bff7194" name="IfcProjectOrder" status="sample" applicableRootEntity="IfcProjectOrder">
          <Applicability uuid="00000000-0000-0000-0000-000000000000" status="sample">
            <Template ref="4a930b17-bb1e-498c-ac7a-2ec6e6c2112d" />
