                <TemplateRule Description="Jacks in order of layout, going to the right and then down, which may accept a cable." Parameters="PredefinedType[Value]='TELEPHONEOUTLET' AND PortName[Value]='Jack#1' AND SystemType[Value]='TELEPHONE' AND Flow[Value]='SINK'" />
