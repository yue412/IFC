        <ConceptRoot uuid="679d4054-4f59-49f0-9b5f-c7494436377b" name="IfcVirtualElement" status="sample" applicableRootEntity="IfcVirtualElement">
          <Applicability uuid="00000000-0000-0000-0000-000000000000" status="sample">
            <Template ref="4372ee5e-83cc-435b-812b-a19bb782ab96" />
